<?xml version="1.0" encoding="utf-8"?>
<project xmlns="http://nant.sf.net/schemas/nant.xsd" name="scratch" default="test" >

  <loadtasks assembly="..\..\Third Party\nAnt\bin\Common.Functions.dll" />
  
  <target name="test">

    <filterchain id="Common.FilterChain">
      <replacestring from="%ProjectName%"  to="ProjectName"   />
    </filterchain>

    <echo message="${filterchain::replacestring-count('Common.FilterChain')}"/>
    
    <property name="VSS.Password" value="password"  overwrite="false"/>
    <function execute="${filterchain::replacestring-add('Common.FilterChain', '%VSS.Password%', VSS.Password)}"/>
    
    <echo message="${filterchain::replacestring-count('Common.FilterChain')}"/>
  </target>

</project>